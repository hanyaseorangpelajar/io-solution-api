        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "try {",
                  "    var res = pm.response.json();",
                  "    if (res && res.token) {",
                  "        pm.collectionVariables.set(\"authToken\", res.token);",
                  "        console.log(\"Auth Token set!\");",
                  "    }",
                  "} catch (e) {",
                  "    console.log(\"Could not parse login response or find token.\");",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"identifier\": \"admin\",\n  \"password\": \"password123\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            },
            "url": {
              "raw": "{{baseUrl}}/auth/login",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "auth",
                "login"
              ]
            },
            "description": "Login untuk mendapatkan token JWT. Endpoint ini publik."
          },
          "response": []
        }
      ],
